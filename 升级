2016-11-30: CREATE VIEW `vip_integration` AS
            SELECT
              NULL as father_id,
              id,
              vip_number,
              point_of_sale,
              consume,
              pay,
              category,
              description,
              self_account,
              group_account,
              pay_serial,
              user_id,
              do_time,
              currency,
              deserve
            FROM vip_detail
            UNION SELECT
              father_id,
              id,
                    vip_number,
                    point_of_sale,
                    consume,
                    pay,
                    category,
                    description,
                    self_account,
                    group_account,
                    pay_serial,
                    user_id,
                    do_time,
                    currency,
                    deserve
                  FROM vip_detail_history;
